{
  "name": "Copy Vertically on Table",
  "description": "Enables to store column's values from HTML table to your clipboard.",
  "version": "0.1",
  "permissions": ["contextMenus","clipboardWrite","clipboardRead"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "all_frames" : true
    }],
  "icons" : { "128" : "icon.png" },
  "manifest_version": 2
}
