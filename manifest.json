{
  "name": "Copy Virtically on Table",
  "description": "Shows some of the features of the Context Menus API",
  "version": "0.6",
  "permissions": ["contextMenus","clipboardWrite","clipboardRead"],
  "background": {
    "scripts": ["ext/jquery-1.11.0.min.js", "background.js"]
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "all_frames" : true
    }],
  "manifest_version": 2
}
